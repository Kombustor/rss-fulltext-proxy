    "rss": "^1.2.2",
    "rss-parser": "^3.7.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.12",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.6",
    "@types/redis": "^2.8.13",
    "@types/request": "^2.48.2",
    "@types/request-promise": "^4.1.44",
    "@types/rss": "0.0.28",
    "@types/xml2js": "^0.4.4",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1",
    "typescript": "^3.5.3"
